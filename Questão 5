#include <stdio.h>


int mcd(int a, int b) {
    if (b == 0) {
        return a;  
    } else {
        return mcd(b, a % b);  
    }
}

int main() {
    int num1 = 56;  
    int num2 = 98;  

    int resultado = mcd(num1, num2);

    printf("O MCD de %d e %d Ã© %d.\n", num1, num2, resultado);

    return 0;
}
